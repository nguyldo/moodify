{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-restricted-syntax": "off",
        "array-callback-return": "off",
        "no-useless-return": "off",
        "consistent-return": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "linebreak-style": "off",
        "prefer-destructuring": "off"
    }
}
